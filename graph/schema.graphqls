type User {
  username: String!
  email: String!
  password: String!
}

type Login {
  email: String!
  password: String!
}

type Register {
  username: String!
  email: String!
  password: String!
  password_confirm: String!
}

type Query {
  users: [User]!
}

input UserInput {
  username: String!
  email: String!
}